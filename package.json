{
  "name": "globeletjs",
  "version": "0.0.1",
  "description": "Lightweight vector maps on a globe",
  "main": "dist/globelet.bundle.js",
  "files": [
    "dist"
  ],
  "directories": {},
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run build-module && npm run build-examples",
    "build-module": "rollup -c build/rollup.config.js",
    "build-examples": "npm run copy-assets & cd examples && rollup -c",
    "copy-assets": "echo examples/*/ | xargs -n 1 cp dist/globelet.css dist/globelet.svg"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GlobeletJS/GlobeletJS.git"
  },
  "keywords": [
    "globe",
    "globelet",
    "webgl",
    "gis",
    "map"
  ],
  "author": "Jeshurun Hembd",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GlobeletJS/GlobeletJS/issues"
  },
  "homepage": "https://globeletjs.org",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^11.2.1",
    "rollup": "^2.46.0"
  },
  "dependencies": {
    "satellite-view": "^2.0.0",
    "spinning-ball": "^0.2.3",
    "tile-setter": "^0.0.4",
    "yawgl": "^0.3.0"
  }
}
